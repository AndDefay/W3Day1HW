for (let i = 1; i <= 20; i ++) {
    console.log(i)
}
//Easy Going answered

for (let i = 0; i <= 200; i += 2) {
    console.log(i)
}
//Get Even answered

for (let i = 1; i <= 100; i++) {
    if (i % 3) {
        console.log('Fizz')
    }
    if (i % 5) {
        console.log('Buzz')
    }
    if ((i % 3) && (i % 5)) {
        console.log('FizzBuzz')
    }
}